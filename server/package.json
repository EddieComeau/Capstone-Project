{
  "name": "nfl-cards-server",
  "version": "1.0.0",
  "description": "NFL Cards backend â€” syncs BallDon'tLie NFL data (players, teams, games, stats, injuries) into MongoDB and computes derived metrics.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "sync": "node scripts/testSync.js",
    "sync:players": "node scripts/testSync.js",
    "sync:all": "node scripts/testSync.js",
    "sync:teams": "node scripts/syncAllTeams.js",
    "sync:data": "node scripts/syncAllData.js",
    "sync:games": "node -e \"require('./services/syncService').syncGames().then(res=>{console.log(res);process.exit(0)}).catch(err=>{console.error(err);process.exit(1)})\"",
    "sync:stats": "node -e \"require('./services/syncService').syncStats().then(res=>{console.log(res);process.exit(0)}).catch(err=>{console.error(err);process.exit(1)})\"",
    "sync:injuries": "node scripts/syncInjuries.js",
    "compute:derived": "node scripts/computeAllDerived.js",
    "compute:matchups": "node -e \"require('./services/derivedService').computeMatchups().then(()=>{console.log('done');process.exit(0)}).catch(err=>{console.error(err);process.exit(1)})\"",
    "test": "echo \"No tests configured\" && exit 0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "nfl",
    "balldontlie",
    "mongodb",
    "sync",
    "players",
